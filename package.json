{
  "name": "@deephaven/web-client-ui",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "scripts": {
    "clean:build": "rimraf packages/*/dist packages/*/build",
    "clean:cache": "run-s clean:cache:*",
    "clean:cache:vite": "rimraf **/node_modules/.vite",
    "clean:root": "rimraf ./node_modules",
    "clean": "run-s clean:*",
    "start": "npm run start --workspace packages/code-studio"
  }
}
